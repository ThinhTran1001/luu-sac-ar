{
  "name": "luu-sac",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:web": "npm run dev -w apps/web",
    "dev:api": "npm run dev -w apps/api",
    "build": "npm run build --workspaces --if-present",
    "build:shared": "npm run build -w packages/shared",
    "build:api": "npm run build -w apps/api",
    "build:web": "npm run build -w apps/web",
    "lint": "npm run lint --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "check": "npm run lint && npm run format",
    "db:generate": "npx prisma generate --schema=apps/api/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=apps/api/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=apps/api/prisma/schema.prisma"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "prisma": "^7.3.0"
  }
}
